				  "name": "Off White"
				},
				{
				  "color": "hsl(0, 100%, 100%)",
				  "slug": "white",
				  "name": "White"
				}
			]
		},
		"spacing": {
		  	"customPadding": true,
		  	"customMargin": true,
		  	"blockGap": true,
		  	"units": [ "em", "rem", "vh", "vw", "vmin", "vmax", "ch" ]
		},
		"layout": {
		  	"contentSize": "var(--wp--custom--layout--default)",
		  	"wideSize": "var(--wp--custom--layout--wide)"
		},
		"border": {
		  	"customColor": true,
		  	"customRadius": true,
		  	"customStyle": true,
		  	"customWidth": true
		}
  	},
  	"styles": {
	  	"spacing": {
		  	"blockGap": "var(--wp--custom--spacing--default)",
		  	"margin": {
			  	"top": "0",
			  	"right": "0",
			  	"bottom": "0",
			  	"left": "0"
			}
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--normal)",
			"fontFamily": "var(--wp--preset--font-family--system)"
		},
		"color": {
			"background": "var(--wp--preset--color--off-white)",
			"text": "var(--wp--preset--color--off-black)"
		},
	  	"elements": {
		  	"link": {},
		  	"h1": {
			  	"typography": {
				  	"fontSize": "var(--wp--preset--font-size--gigantic)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--huge)"
			  }
			},
			"h3": {
			  	"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-large)"
			  }
			},
			"h4": {
			  	"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
			  }
			},
			"h5": {
			  	"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
			  }
			},
			"h6": {
			  	"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
			  }
			}
		},
	  	"blocks": {
		  	"core/buttons": {
				"typography": {
				  	"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
		  	"core/list": {
			}
		}
	},
	"templateParts": [
	  	{
			"name": "header",
			"area": "header"
	  	}
	]
