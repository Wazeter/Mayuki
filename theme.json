{
  "version": 1,
  "settings": {
		"custom": {
		  	"ratio": 1.25,
		  	"measure": "65ch",
			"base": "1rem",
			"s1": "calc(var(--wp--custom--base) * var(--wp--custom--ratio))",
			"s2": "calc(var(--wp--custom--s1) * var(--wp--custom--ratio))",
			"s3": "calc(var(--wp--custom--s2) * var(--wp--custom--ratio))",
			"s4": "calc(var(--wp--custom--s3) * var(--wp--custom--ratio))",
			"s5": "calc(var(--wp--custom--s4) * var(--wp--custom--ratio))",
			"s-1": "calc(var(--wp--custom--base) / var(--wp--custom--ratio))",
			"s-2": "calc(var(--wp--custom--s-1) / var(--wp--custom--ratio))",
			"s-3": "calc(var(--wp--custom--s-2) / var(--wp--custom--ratio))",
			"s-4": "calc(var(--wp--custom--s-3) / var(--wp--custom--ratio))",
			"s-5": "calc(var(--wp--custom--s-4) / var(--wp--custom--ratio))"
		},
		"typography": {
		  	"fontSizes": [
				{
				  "slug": "tiny",
				  "size": "clamp(var(--wp--custom--s-2), calc(var(--wp--custom--s-1) + 1vmin), var(--wp--custom--s-1))",
				  "name": "Tiny"
				},
				{
				  "slug": "small",
				  "size": "clamp(var(--wp--custom--s-1), calc(var(--wp--custom--base) * 0.75 + 1vmin), var(--wp--custom--base))",
				  "name": "Small"
				},
				{
				  "slug": "normal",
				  "size": "clamp(var(--wp--custom--base), calc(var(--wp--custom--base) + 0.5vmin), var(--wp--custom--s1))",
				  "name": "Normal"
				},
				{
				  "slug": "medium",
				  "size": "clamp(var(--wp--custom--base), calc(var(--wp--custom--s1) * 0.75 + 1vmin), calc(var(--wp--custom--s1) + 1vmin))",
				  "name": "Medium"
				},
				{
				  "slug": "large",
				  "size": "clamp(var(--wp--custom--s1), calc(var(--wp--custom--s1) + 1vmin), var(--wp--custom--s2))",
				  "name": "Large"
				},
				{
				  "slug": "extra-large",
				  "size": "clamp(var(--wp--custom--s1), calc(var(--wp--custom--s1) + 1.5vmin), var(--wp--custom--s3))",
				  "name": "Extra Large"
				},
				{
				  "slug": "huge",
				  "size": "clamp(var(--wp--custom--s2), calc(var(--wp--custom--s2) + 2vmin), var(--wp--custom--s4))",
				  "name": "Huge"
				},
				{
				  "slug": "Gigantic",
				  "size": "clamp(var(--wp--custom--s3), calc(var(--wp--custom--s3) + 3vmin), var(--wp--custom--s5))",
				  "name": "Gigantic"
				}
			],
		  	"fontFamilies": [
				{
				  "fontFamily": "-apple-system, BlinkMacSystemFont, avenir next, avenir, segoe ui, helvetica neue, helvetica, Ubuntu, roboto, noto, arial, sans-serif",
				  "slug": "system",
				  "name": "System Font"
				},
				{
				  "fontFamily": "Iowan Old Style, Apple Garamond, Baskerville, Times New Roman, Droid Serif, Times, Source Serif Pro, serif, Apple Color Emoji, Segoe UI Emoji, Segoe UI Symbol",
				  "slug": "serif-system",
				  "name": "Serif Font"
				},
				{
				  "fontFamily": "Menlo, Consolas, Monaco, Liberation Mono, Lucida Console, monospace",
				  "slug": "monospace",
				  "name": "Monospace"
				}
			],
		  	"customLineHeight": true
		},
		"color": {
			"palette": [
				{
				  "color": "hsl(194, 60%, 53%)",
				  "slug": "brand-primary",
				  "name": "Brand Primary Color"
				},
				{
				  "color": "hsl(355, 59%, 39%)",
				  "slug": "brand-secondary",
				  "name": "Brand Secondary Color"
				},
				{
				  "color": "hsl(0, 0%, 15%)",
				  "slug": "off-black",
				  "name": "Off Black"
				},
				{
				  "color": "hsl(0, 0%, 0%)",
				  "slug": "black",
				  "name": "Black"
				},
				{
				  "color": "hsl(240, 33%, 99%)",
				  "slug": "off-white",
				  "name": "Off White"
				},
				{
				  "color": "hsl(0, 100%, 100%)",
				  "slug": "white",
				  "name": "White"
				},
				{
				  "color": "hsl(207, 17%, 72%)",
				  "slug": "cyan-bluish-gray",
				  "name": "Cyan Bluish Gray"
				},
			  	{
				  "color": "hsl(0, 0%, 93%)",
				  "slug": "silver",
				  "name": "Silver"
				}
			]
		},
		"spacing": {
		  	"customPadding": true,
		  	"customMargin": true,
		  	"units": [ "em", "rem", "vh", "vw", "vmin", "vmax", "ch" ]
		},
		"layout": {
		  	"contentSize": "clamp(35rem, calc(100% - 30rem), 60vw)",
		  	"wideSize": "clamp(50rem, calc(100% - 15rem), 80vw)"
		},
		"border": {
		  	"customColor": true,
		  	"customRadius": true,
		  	"customStyle": true,
		  	"customWidth": true
		}
  	},
  	"styles": {
	  	"typography": {
		  	"fontSize": "var(--wp--custom--s1)",
		  	"fontFamily": "var(--wp--preset--font-family--system)"
		},
	  	"color": {
		  	"background": "var(--wp--preset--color--off-white)",
		  	"text": "var(--wp--preset--color--off-black)"
		}
	}
}