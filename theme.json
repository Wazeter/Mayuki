{
	"version": 1,
	"templateParts": [
		{
			"name": "header",
			"area": "header"
		},
		{
			"name": "footer",
			"area": "footer"
		}
	],
	"settings": {
		"layout": {
			"contentSize": "42em",
			"wideSize": "52em"
		},
		"spacing": {
			"customMargin": true,
			"customPadding": true,
			"units": [ "rem", "em", "px", "vh", "vw" ]
		},
		"border": {
			"customColor": true,
			"customRadius": true,
			"customStyle": true,
			"customWidth": true
		},
		"color": {
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"link": true,
		  "palette": [
			{
				"color": "hsl(194, 60%, 53%)",
				"slug": "mayuki-teal",
				"name": "Mayuki Teal"
			},
			{
				"color": "hsl(355, 59%, 39%)",
				"slug": "mayuki-crimson",
				"name": "Mayuki Crimson"
			},
			{
				"color": "hsl(0, 0%, 15%)",
				"slug": "off-black",
				"name": "Off Black"
			},
			{
				"color": "hsl(207, 17%, 72%)",
				"slug": "cyan-bluish-gray",
				"name": "Cyan Bluish Gray"
			},
			{
				"color": "hsl(198, 12.4%, 73.1%)",
				"slug": "gray",
				"name": "Gray"
			},
			{
				"color": "hsl(240, 33%, 99%)",
				"slug": "off-white",
				"name": "Off White"
			},
			{
				"color": "hsl(0, 0%, 0%)",
				"slug": "black",
				"name": "Black"
			},
			{
				"color": "hsl(0, 100%, 100%)",
				"slug": "white",
				"name": "White"
			},
			{
			  	"color": "hsla(360, 0%, 0%, 0)",
			  	"slug": "transparent",
			  	"name": "Transparent"
			}
		  ],
		  "gradients" : []
		},
		"typography": {
			"customFontSize": true,
			"customFontStyle": true,
			"customFontWeight": true,
			"customLineHeight": true,
			"customTextDecorations": true,
			"customTextTransforms": true,
			"dropCap": true,
			"fontFamilies": [
			  {
				  "fontFamily": "-apple-system, BlinkMacSystemFont, avenir next, avenir, segoe ui, helvetica neue, helvetica, Ubuntu, roboto, noto, arial, sans-serif",
				  "slug": "system",
				  "name": "System Font"
			  },
			  {
				  "fontFamily": "Iowan Old Style, Apple Garamond, Baskerville, Times New Roman, Droid Serif, Times, Source Serif Pro, serif, Apple Color Emoji, Segoe UI Emoji, Segoe UI Symbol",
				  "slug": "serif-system",
				  "name": "Serif Font"
			  },
			  {
				  "fontFamily": "Menlo, Consolas, Monaco, Liberation Mono, Lucida Console, monospace",
				  "slug": "monospace",
				  "name": "Monospace"
			  }
			],
			"fontSizes": [
			  {
				  "slug": "small",
				  "size": "1rem",
				  "name": "Small"
			  },
			  {
				  "slug": "normal",
				  "size": "1.125rem",
				  "name": "Normal"
			  },
			  {
				  "slug": "medium",
				  "size": "1.3125rem",
				  "name": "Medium"
			  },
			  {
				  "slug": "large",
				  "size": "2.125rem",
				  "name": "Large"
			  },
			  {
				  "slug": "huge",
				  "size": "3rem",
				  "name": "Huge"
			  }
			]
		},
		"custom": {
		  	"spacing": {
			  "horizontal": "1.25rem",
			  "vertical": "1.25rem"
			},
			"color": {
				"primary": "var(--wp--preset--color--mayuki-teal)",
				"secondary": "var(--wp--preset--color--mayuki-crimson)",
			  	"heading": "var(--wp--custom--color--primary)",
			  	"text": "var(--wp--preset--color--off-black)"
			},
			"fonts": {
				"primary": "var(--wp--preset--font-family--system)",
				"secondary": "var(--wp--preset--font-family--serif-system)",
				"code": "var(--wp--preset--font-family--monospace)",
			  	"heading": "var(--wp--custom--fonts--primary)",
			  	"text": "var(--wp--custom--fonts--primary)"
			},
			"lineHeight": {
				"heading": 1.2,
				"body": 1.5
			}
		},
		"blocks": {
		}
	},
	"styles": {
		"spacing": {
			"margin": {
				"top": "0px",
				"right": "0px",
				"bottom": "0px",
				"left": "0px"
			}
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--off-black)"
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontFamily": "var(--wp--custom--fonts--primary)",
			"lineHeight": "var(--wp--custom--line-height--body)"
		},
		"elements": {
			"link": {
				"typography": {
				  	"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--huge)",
					"lineHeight": "var(--wp--custom--line-height--heading)"
				}
			},
			"h2": {
			  	"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "var(--wp--custom--line-height--heading)"
			  	}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "var(--wp--custom--line-height--heading)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			}
		},
		"blocks": {
			"core/button": {
				"spacing": {
					"margin": {
					  	"right": "var(--wp--custom--spacing--horizontal)"
					}
				},
				"border": {
					"color": "var(--wp--custom--color--primary)",
					"radius": "5px"
				},
				"color": {
				  	"background": "var(--wp--custom--color--primary)"
				}
			},
		  	"core/paragraph": {
			  	"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				  }
			},
		  	"core/code": {
				"color": {
				  	"background": "var(--wp--preset--color--gray)"
				}
			}
		}
	}
}